        "cheats",
        "patch",
        "savegame",
        "savestate",
        "screenshot",
        "shaders",
        "qt.ini",
        "config.ini",
        "portable.ini"
    ],
    "checkver": {
        "url": "https://mgba.io/builds/1/",
        "regex": "build-(?<year>20\\d{2})-(?<month>\\d{2})-(?<day>\\d{2})-win32-(?<ver>[\\d]+)-(?<git>\\w{40})\\.7z",
        "replace": "$4-$1$2$3"
    },
    "autoupdate": {
        "architecture": {
            "32bit": {
                "url": "https://s3.amazonaws.com/mgba/build/mGBA-build-$matchYear-$matchMonth-$matchDay-win32-$matchVer-$matchGit.7z",
