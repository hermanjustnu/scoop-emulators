        "replace": "${version}-${build}-${commit}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v$matchVersion-$matchBuild-g$matchCommit&platform=windows-amd64#/dl.zip"
